      "qc@": "@qq.com",
      "gc@": "@gmail.com",
      "16@": "@163.com"
    },
    "custom": {
    }
  },
  "hot-corner": {
    "action": {
      "LeftTopCorner": {
        "hover": "JumpToPrevTab"
      },
      "RightTopCorner": {
        "hover": "JumpToNextTab"
      },
      "LeftBottomCorner": {
        "hover": "Win"
      },
      "RightBottomCorner": {
        "hover": "GotoPreApp"
      }
    }
  },
  "hot-edge": {
    "enable": 1,
    "action": {
      "TopHalfLeftEdge": {
        "ctrl_8": "C:\\Program Files (x86)\\Netease\\CloudMusic\\cloudmusic.exe",
        "wheeldown": "GoBottom",
        "wheelup": "GoTop",
        "MButton": "MinWindow"
      },
      "TopHalfRightEdge": {
        "ctrl_8": "OpenFileExplorer",
        "wheeldown": "PgDn",
        "wheelup": "PgUp",
        "MButton": "MaxWindow"
      },
      "BottomHalfLeftEdge": {
        "ctrl_8": "ReopenLastTab",
        "MButton": "OpenTaskView",
        "wheeldown": "GotoNextDesktop",
        "wheelup": "GotoPreDesktop"
      },
      "BottomHalfRightEdge": {
        "ctrl_8": "RefreshTab",
